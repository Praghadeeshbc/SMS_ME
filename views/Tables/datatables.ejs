            type="text/javascript"
            src="https://cdn.datatables.net/v/dt/dt-1.10.24/datatables.min.js"
        ></script>

        <!-- Sweet Alert -->
        <script src="/js/plugin/sweetalert/sweetalert.min.js"></script>

        <!-- Atlantis JS -->
        <script src="/js/atlantis.min.js"></script>

        <script>
            //change true when hosting
            let isTrue = false;
            let url = isTrue
                ? "https://sms-med.herokuapp.com/api/logs"
                : "/api/logs";
            $("#myTable").DataTable({
                order: [[3, "desc"]],
                lengthMenu: [10, 25, 50],
                ajax: {
                    url: url,
                    dataSrc: "",
                },
                columns: [
                    { data: "spokeid" },
                    { data: "hubid" },
                    { data: "ontime" },
                    { data: "offtime", defaultContent: "<b>--:--:--</b>" },
                    {
                        data: "date",
                        render: (data, type, full, meta) =>
                            `${full.day}, ${full.date}`,
                    },
                ],
            });
        </script>
